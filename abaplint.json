{
   "rules":{
      "7bit_ascii":{
         "enabled":true
      },
      "contains_tab":{
         "enabled":true
      },
      "functional_writing":{
         "enabled":true
      },
      "line_length":{
         "enabled":true,
         "length":100
      },
      "max_one_statement":{
         "enabled":true
      },
      "parser_error":{
         "enabled":false
      },
      "space_before_colon":{
         "enabled":true
      },
      "colon_missing_space":{
         "enabled":true
      },
      "exit_or_check":{
         "enabled":false
      },
      "line_only_punc":{
         "enabled":false
      },
      "obsolete_statement":{
         "enabled":true
      },
      "start_at_tab":{
         "enabled":true
      },
      "whitespace_end":{
         "enabled":true
      },
      "exporting":{
         "enabled":true
      },
      "empty_statement":{
         "enabled":true
      },
      "sequential_blank":{
         "enabled":true,
         "lines":4
      },
      "definitions_top":{
         "enabled":false
      },
      "indentation":{
         "enabled":true
      }
   }
}
